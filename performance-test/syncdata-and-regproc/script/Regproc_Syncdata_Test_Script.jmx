              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">import java.nio.file.Files
import java.nio.file.Paths
import java.security.KeyFactory
import java.security.PublicKey
import java.security.spec.X509EncodedKeySpec
import java.util.Base64

// Path to your PEM file
def keyPath = vars.get(&quot;keyPath&quot;)
def countNum = vars.get(&quot;countNum&quot;).toInteger().toString()
def pemPath = keyPath + &quot;/reg_&quot; + countNum

// Read and clean the PEM file
def pemContent = new String(Files.readAllBytes(Paths.get(pemPath + &quot;.pem&quot;)), &quot;UTF-8&quot;)

// Remove header, footer, and whitespace
def cleaned = pemContent
    .replaceAll(&quot;-----BEGIN PUBLIC KEY-----&quot;, &quot;&quot;)
    .replaceAll(&quot;-----END PUBLIC KEY-----&quot;, &quot;&quot;)
    .replaceAll(&quot;\\s&quot;, &quot;&quot;)

// Log to confirm
//log.info(&quot;Public key loaded successfully: &quot; + cleaned)
//log.info(&quot;Key Path: &quot; + pemPath)

//Save to variable
vars.put(&quot;pemPath&quot;, pemPath)
vars.put(&quot;pubKey&quot;, cleaned)
</stringProp>
