            <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="Request Token Generation using keys" enabled="true">
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="script">import java.security.KeyFactory
import java.security.PrivateKey
import java.security.PublicKey
import java.security.Signature
import java.security.spec.PKCS8EncodedKeySpec
import java.security.spec.X509EncodedKeySpec
import java.security.MessageDigest
import java.time.LocalDateTime
import java.time.ZoneOffset
import java.time.format.DateTimeFormatter
import org.apache.commons.codec.binary.Hex
import java.time.LocalDateTime
import java.time.ZoneOffset
import java.time.format.DateTimeFormatter
import java.nio.file.NoSuchFileException
import java.security.NoSuchAlgorithmException
import java.security.spec.InvalidKeySpecException


// Define file paths for private and public keys (change these to your actual file locations)
def keyPath = vars.get(&quot;pemPath&quot;)

String privateKeyPath = keyPath + &quot;.key&quot;  // Path to private key file
String publicKeyPath = keyPath + &quot;.pub&quot;   // Path to public key file

// Load private key
byte[] privateBytes = new File(privateKeyPath).bytes
PKCS8EncodedKeySpec privateSpec = new PKCS8EncodedKeySpec(privateBytes)
KeyFactory kf = KeyFactory.getInstance(&quot;RSA&quot;)
PrivateKey privateKey = kf.generatePrivate(privateSpec)
// Load public key
byte[] publicBytes = new File(publicKeyPath).bytes
X509EncodedKeySpec publicSpec = new X509EncodedKeySpec(publicBytes)
PublicKey publicKey = kf.generatePublic(publicSpec)



// Updated computeFingerPrint to match CryptoUtil exactly (using SHA-256 digest, as per MOSIP patterns; metaData null)
String computeFingerPrint(byte[] data, String metaData) {
    byte[] combinedPlainTextBytes = data
    if (metaData != null &amp;&amp; !metaData.isEmpty()) {
        // Replicate ArrayUtils.addAll(data, metaData.getBytes())
        combinedPlainTextBytes = new byte[data.length + metaData.getBytes().length]
        System.arraycopy(data, 0, combinedPlainTextBytes, 0, data.length)
        System.arraycopy(metaData.getBytes(), 0, combinedPlainTextBytes, data.length, metaData.getBytes().length)
    }
    // Compute hash: Assuming HMACUtils.generateHash is SHA-256 digest (unkeyed, as no key provided; matches PKI fingerprint)
    MessageDigest md = MessageDigest.getInstance(&quot;SHA-256&quot;)
    byte[] hash = md.digest(combinedPlainTextBytes)
    // Hex.encodeHexString (lowercase hex)
    String hexString = Hex.encodeHexString(hash)
    // Insert colons every 2 chars: replaceAll(&quot;..(?!$)&quot;, &quot;$0:&quot;)
    return hexString.replaceAll(&apos;..(?!$)&apos;, &apos;$0:&apos;)
    //return hexString.replaceAll(&quot;..(?!$)&quot;, &quot;$0:&quot;)
}

String kid = computeFingerPrint(publicBytes, null)

// Timestamp
String timestamp = LocalDateTime.now(ZoneOffset.UTC).minusMinutes(1).format(DateTimeFormatter.ofPattern(&quot;yyyy-MM-dd&apos;T&apos;HH:mm:ss.SSS&apos;Z&apos;&quot;))
// Payload
String payload = &quot;{\&quot;userId\&quot; : \&quot;performance\&quot;, \&quot;password\&quot;: \&quot;mosip123\&quot;, \&quot;authType\&quot;:\&quot;NEW\&quot;, \&quot;timestamp\&quot; : \&quot;&quot;+timestamp+&quot;\&quot;}&quot;
// Header
String header = &quot;{\&quot;kid\&quot; : \&quot;&quot; + kid + &quot;\&quot;}&quot;
// Sign data
Signature sign = Signature.getInstance(&quot;SHA256withRSA&quot;)
sign.initSign(privateKey)
sign.update(payload.getBytes(&quot;UTF-8&quot;))
byte[] signatureByte = sign.sign()
// Encode to URL-safe Base64 without padding
def encodeToURLSafeBase64(byte[] bytes) {
return java.util.Base64.getUrlEncoder().withoutPadding().encodeToString(bytes)
}
String headerCrypto = encodeToURLSafeBase64(header.getBytes(&quot;UTF-8&quot;))
String payloadCrypto = encodeToURLSafeBase64(payload.getBytes(&quot;UTF-8&quot;))
String signCrypto = encodeToURLSafeBase64(signatureByte)
String completeCrypto = headerCrypto + &quot;.&quot; + payloadCrypto + &quot;.&quot; + signCrypto

// Set variables
vars.put(&quot;timeStamp&quot;, timestamp)
vars.put(&quot;requestToken&quot;, completeCrypto)
// Note: Adjusted timestamp with -1 minute as in original __timeShift with -PT0H01M</stringProp>
